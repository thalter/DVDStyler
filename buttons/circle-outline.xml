<?xml version="1.0" encoding="utf-8"?>
<button previewHighlighted="true">
	<title>Circle outline</title>
	<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
			width="50" height="50" viewBox="0 0 28 28">
	    <circle id="circle" r="13" cy="14" cx="14" style="fill:#ffff00;" />
	    <circle id="circleOutline" r="13" cy="14" cx="14" style="fill:none" stroke-width="1" />
	    <text
	       id="text" y="14" x="14"
	       style="font-size:10px;fill:#000080"
	       text-anchor="middle" dominant-baseline="middle"
	       xml:space="preserve">01</text>
	</svg>
	<parameters>
		<parameter>
			<title>Label</title>
			<name>text</name>
			<type>text</type>
			<element>text</element>
		</parameter>
		<parameter>
			<title>Circle colour</title>
			<name>circle_fill</name>
			<type>colour</type>
			<element>circle</element>
			<attribute>fill</attribute>
		</parameter>
		<parameter>
			<title>Text colour</title>
			<name>text_fill</name>
			<type>colour</type>
			<element>text</element>
			<attribute>fill</attribute>
		</parameter>
		<parameter>
			<title>Outline colour</title>
			<name>stroke</name>
			<type>colour</type>
			<element>circleOutline</element>
			<attribute>stroke</attribute>
			<changeable/>
			<default-value>
				<highlighted>#FF0000</highlighted>
			 	<selected>#FFFFFF</selected>
			</default-value>
		</parameter>
		<parameter>
			<title>Outline width</title>
			<name>stroke-width</name>
			<type>integer</type>
			<element>circleOutline</element>
			<attribute>stroke-width</attribute>
		</parameter>
	</parameters>
</button>