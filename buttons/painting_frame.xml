<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!-- Created by Gustavo Rezende -->
<button>
<title>Painting Frame</title>
<svg width="200" height="150" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="-2 -2 204 154">
  <defs>
    <linearGradient id="base">
      <stop id="st1" stop-color="#333333" stop-opacity="0.5" offset="0" />
      <stop id="st2" stop-color="#ffffff" stop-opacity="0.5" offset="0.26" />
      <stop id="st3" stop-color="#000000" stop-opacity="0.5" offset="0.6" />
      <stop id="st4" stop-color="#cccccc" stop-opacity="0.5" offset="0.7" />
      <stop id="st5" stop-color="#333333" stop-opacity="0.5" offset="0.73" />
      <stop id="st6" stop-color="#333333" stop-opacity="0.5" offset="0.76" />
      <stop id="st7" stop-color="#cccccc" stop-opacity="0.5" offset="0.81" />
      <stop id="st8" stop-color="#000000" stop-opacity="0.5" offset="0.86" />
      <stop id="st9" stop-color="#000000" stop-opacity="0.5" offset="0.89" />
      <stop id="st10" stop-color="#cccccc" stop-opacity="0.5" offset="0.93" />
      <stop id="st11" stop-color="#333333" stop-opacity="0.5" offset="1" />
    </linearGradient>
    <linearGradient id="grad1" x1="0" x2="25" y1="75" y2="75" xlink:href="#base" gradientUnits="userSpaceOnUse" />
    <linearGradient id="grad2" x1="100" x2="100" y1="0" y2="25" xlink:href="#base" gradientUnits="userSpaceOnUse" />
    <linearGradient id="grad3" x1="200" x2="175" y1="75" y2="75" xlink:href="#base" gradientUnits="userSpaceOnUse" />
    <linearGradient id="grad4" x1="100" x2="100" y1="150" y2="125" xlink:href="#base" gradientUnits="userSpaceOnUse" />
    <clipPath id="clip" clipPathUnits="userSpaceOnUse">
      <rect x="20" y="20" width="160" height="110" fill="none" stroke="none" />
    </clipPath>
  </defs>
  <g>
    <image id="image" x="15" y="15" width="170" height="120" stroke="none" opacity="1.0" clip-path="url(#clip)" preserveAspectRatio="xmidymid slice" />
    <path id="frame" fill="#ff9900" stroke="none" d="m 0,0 0,150 200,0 L 200,0 0,0 z m 25,25 150,0 0,100 -150,0 0,-100 z" />
    <path id="frameSel" fill="none" stroke="none" stroke-width="2" d="m 0,0 0,150 200,0 L 200,0 0,0 z" />
    <path id="frame1" fill="url(#grad1)" stroke="none" d="m 0,5e-6 25,25 0,100 L 0,150 z" />
    <path id="frame2" fill="url(#grad2)" stroke="none" d="m 200,0 -25,25 -150,0 L 0,0 z" />
    <path id="frame3" fill="url(#grad3)" stroke="none" d="m 200,2.5e-6 -25,25 0,100 25,25 z" />
    <path id="frame4" fill="url(#grad4)" stroke="none" d="m 200,150 -25,-25 -150,0 L 0,150 z" />
  </g>
</svg>
<parameters>
    <parameter>
      <title>Image</title>
      <name>href</name>
      <type>image</type>
      <element>image</element>
      <attribute>xlink:href</attribute>
    </parameter>
    <parameter>
      <title>Frame colour</title>
      <name>fc</name>
      <type>colour</type>
      <element>frameSel</element>
      <attribute>stroke</attribute>
      <changeable />
      <default-value>
        <highlighted>#FF0000</highlighted>
        <selected>#FFFF00</selected>
      </default-value>
    </parameter>
  </parameters>
  <init-parameter>href</init-parameter>
  <default-size>
    <width>200</width>
    <height>150</height>
  </default-size>
  <min-size>
    <width>15</width>
    <height>15</height>
  </min-size>
</button>