        <stop stop-color="#ffffff" stop-opacity="1" offset="0" />
        <stop stop-color="#ffffff" stop-opacity="0.8" offset="0.2" />
        <stop stop-color="#ffffff" stop-opacity="0" offset="0.4" />
        <stop stop-color="#000000" stop-opacity="0.4" offset="1" />
