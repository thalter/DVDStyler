    <parameter>
      <title>Note</title>
      <name>note</name>
      <type>colour</type>
      <element>note</element>
      <attribute>fill</attribute>
    </parameter>
    <parameter>
      <title>Pin</title>
      <name>pin</name>
      <type>colour</type>
      <element>ball1</element>
      <attribute>fill</attribute>
    </parameter>
    <parameter>
      <title>Selection colour</title>
      <name>sel</name>
      <type>colour</type>
      <element>sel</element>
      <attribute>stroke</attribute>
      <changeable />
